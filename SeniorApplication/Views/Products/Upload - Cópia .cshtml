<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div id="body">
        <form name="form1" method="post" action="~/api/uploadfile" enctype="multipart/form-data">
            <div>
                <label>
                    Upload file
                </label>
                <input name="myFile" type="file" accept=".csv"/>
            </div>
            <div>
                <input type="submit" value="Import"/>
            </div>
        </form>
    </div>
</body>
</html>